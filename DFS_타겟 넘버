#include <string>
#include <vector>

using namespace std;

int solution(vector<int> numbers, int target) {
    int answer = 0, num = 0;
    num = numbers.front();
    if (numbers.size() == 0)
        if (target == 0)
            return 1;
        else
            return 0;
    else {
        numbers.erase(numbers.begin());
        return solution(numbers, target - num) + solution(numbers, target + num);
    }
}
