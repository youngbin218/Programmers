#include <iostream>
#include <algorithm>

using namespace std;

int Answer;

int main(int argc, char** argv)
{
	int T, test_case;
	int N, K;
	int A[200000];
	int B[200000];

	cin >> T;
	
	for(test_case = 0; test_case < T; test_case++)
	{
	    cin >> N >> K;
	    for (int i=0;i<N;i++)
	        cin >> A[i];
	    for (int i=0;i<N;i++)
	        cin >> B[i];

		Answer = 0;
		
		sort(A, A+N);
		sort(B, B+N);
		
		for (int i=0;i<K;i++)
		    Answer = max(Answer, A[i]+B[K-i-1]);
		
		cout << "Case #" << test_case+1 << endl;
		cout << Answer << endl;
	}

	return 0;
}
